@model Evaluacion2.Models.UsuarioModel

<h2>Detalles del Usuario</h2>

<dl class="row">
    <dt class="col-sm-2">
        Nombre de Usuario
    </dt>
    <dd class="col-sm-10">
        @Model.NombreDeUsuario
    </dd>
    <dt class="col-sm-2">
        Nombre Completo
    </dt>
    <dd class="col-sm-10">
        @Model.NombreCompleto
    </dd>
    <dt class="col-sm-2">
        Edad
    </dt>
    <dd class="col-sm-10">
        @Model.Edad
    </dd>
    <dt class="col-sm-2">
        Correo
    </dt>
    <dd class="col-sm-10">
        @Model.Correo
    </dd>
</dl>

<a href="@Url.Action("Index")" class="btn btn-secondary">Volver a la Lista</a>
